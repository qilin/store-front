{
  "name": "qilin-web-store",
  "version": "0.1.4-alpha",
  "author": {
    "name": "Qilin Team",
    "email": "your.email@domain.com",
    "url": "https://your-website.com"
  },
  "description": "Qilin Web Store",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@material-ui/core": "^4.7.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.6.0",
    "@types/jest": "24.0.18",
    "@types/node": "13.5.0",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-router-dom": "^5.1.3",
    "apollo-boost": "^0.4.4",
    "graphql": "^14.5.8",
    "history": "^4.10.1",
    "i18next": "^19.0.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-i18next": "^11.2.5",
    "react-responsive-carousel": "^3.1.51",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "typeface-roboto": "^0.0.75",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "codegen:typescript": "graphql-codegen --config codegen.yml",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "start:renderer": "REACT_APP_MODE=electron BROWSER=none yarn start",
    "build:renderer": "REACT_APP_MODE=electron rescripts build && yarn cp:static",
    "create:dist": "mkdir -p ./../launcher/app/dist",
    "cp:static": "yarn create:dist && yarn cp:css && yarn cp:js && yarn cp:media",
    "cp:css": "cp build/static/css/*.css ./../launcher/app/dist/style.css",
    "cp:js": "cp build/static/js/*.js ./../launcher/app/dist/renderer.js",
    "cp:media": "cp -r build/static/media ./../launcher/app/dist/static",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint './**/*.{ts,tsx}'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.9.1",
    "@graphql-codegen/typescript": "^1.9.1",
    "@graphql-codegen/typescript-operations": "^1.9.1",
    "@rescripts/cli": "^0.0.13"
  }
}
