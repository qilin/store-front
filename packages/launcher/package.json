{
  "name": "qilin-launcher",
  "version": "0.1.9-alpha",
  "private": true,
  "homepage": "./",
  "main": "electron/main.js",
  "author": {
    "name": "Qilin Team",
    "email": "your.email@domain.com",
    "url": "https://your-website.com"
  },
  "description": "Qilin Store Launcher",
  "dependencies": {
    "@qilin/shared": "*",
    "@apollo/react-hooks": "^3.1.3",
    "@material-ui/core": "^4.7.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.6.0",
    "@types/jest": "24.0.18",
    "electron-is-dev": "1.1.0",
    "electron-log": "4.0.3",
    "electron-updater": "4.2.0",
    "@types/node": "13.5.0",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-router-dom": "^5.1.3",
    "apollo-boost": "^0.4.4",
    "graphql": "^14.5.8",
    "i18next": "^19.0.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-i18next": "^11.2.5",
    "react-responsive-carousel": "^3.1.51",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "typeface-roboto": "^0.0.75",
    "typescript": "3.6.3"
  },
  "scripts": {
    "start": "concurrently \"yarn start-renderer\" \"wait-on http://localhost:3000 && yarn start-main\"",
    "start-renderer": "BROWSER=none react-app-rewired start",
    "start-main": "yarn electron .",
    "build": "react-app-rewired build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint './**/*.{ts,tsx}'",
    "postinstall": "electron-builder install-app-deps",
    "build-electron": "mkdir build/src && cp -r electron/. build/electron && cp src/ipc.constants.js build/src/ipc.constants.js",
    "preelectron-pack": "yarn build && yarn build-electron",
    "electron-pack": "electron-builder build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "electron": "7.1.9",
    "electron-builder": "22.3.2",
    "wait-on": "^4.0.0",
    "customize-cra": "^0.9.1",
    "react-app-rewired": "^2.1.5"
  }
}
