{
  "name": "qilin-launcher",
  "version": "1.0.2",
  "main": "./build/main.js",
  "author": {
    "name": "Qilin Team",
    "email": "your.email@domain.com",
    "url": "https://your-website.com"
  },
  "description": "Qilin Store Launcher",
  "license": "MIT",
  "dependencies": {
    "@sentry/electron": "^1.2.0",
    "electron-debug": "^3.0.1",
    "electron-log": "4.0.3",
    "electron-updater": "4.2.0"
  },
  "scripts": {
    "prestart": "yarn build",
    "dev": "yarn start-main-dev",
    "start": "cross-env NODE_ENV=production yarn electron ./build/main.js",
    "build": "cross-env NODE_ENV=production yarn build-main",
    "start-main-dev": "cross-env NODE_ENV=development concurrently \"webpack --watch --colors\" \"yarn electron ./build/main.js\"",
    "build-main": "webpack --colors",
    "electron-pack": "yarn build && electron-builder build",
    "lint": "eslint './**/*.{ts,tsx}'"
  },
  "devDependencies": {
    "@types/node": "13.5.0",
    "concurrently": "^5.0.2",
    "cross-env": "^7.0.0",
    "electron": "7.1.9",
    "electron-builder": "21.2.0",
    "terser-webpack-plugin": "^2.3.2",
    "ts-loader": "^6.2.1",
    "typescript": "3.6.3",
    "webpack": "4.41.2",
    "webpack-merge": "^4.2.2",
    "webpack-cli": "3.3.10"
  }
}
