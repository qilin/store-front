{
  "name": "qilin-launcher",
  "version": "0.1.4-alpha",
  "private": true,
  "homepage": "./",
  "main": "electron/main.js",
  "author": {
    "name": "Qilin Team",
    "email": "your.email@domain.com",
    "url": "https://your-website.com"
  },
  "description": "Qilin Store Launcher",
  "dependencies": {
    "@qilin/shared-components": "0.0.1",
    "@apollo/react-hooks": "^3.1.3",
    "@material-ui/core": "^4.7.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.6.0",
    "@types/jest": "24.0.18",
    "electron-is-dev": "1.1.0",
    "electron-log": "4.0.3",
    "electron-updater": "4.2.0",
    "@types/node": "12.7.5",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-router-dom": "^5.1.3",
    "apollo-boost": "^0.4.4",
    "graphql": "^14.5.8",
    "i18next": "^19.0.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-i18next": "^11.2.5",
    "react-responsive-carousel": "^3.1.51",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.1.2",
    "typeface-roboto": "^0.0.75",
    "typescript": "3.6.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint './**/*.{ts,tsx}'",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "build-electron": "mkdir build/src && cp -r electron/. build/electron && cp src/ipc.constants.js build/src/ipc.constants.js",
    "preelectron-pack": "yarn build && yarn build-electron",
    "electron-pack": "electron-builder build -m -c.extraMetadata.main=build/electron/main.js -c.mac.identity=null --publish never"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{ts,tsx}": [
      "yarn lint",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "customize-cra": "^0.9.1",
    "eslint": "^6.4.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.5",
    "lint-staged": "^9.3.0",
    "concurrently": "^5.0.2",
    "electron": "7.1.9",
    "electron-builder": "21.2.0",
    "wait-on": "^4.0.0",
    "react-app-rewired": "^2.1.5"
  }
}
